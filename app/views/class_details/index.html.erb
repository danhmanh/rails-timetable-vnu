<h1>Crawl dữ liệu thành công.</h1>

<h1>Bạn đã đăng kí tổng cộng <%= current_user.courses.count %> môn học</h1>
<table class="table table-bordered" style="table-layout: fixed;">
  <thead>
    <tr>
      <th></th>
      <% 1.upto(10) do |index| %>
        <th><%= index %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% 2.upto(7) do |dow| %>
      <tr>
        <td>Thứ <%= dow %></td>
        <% 1.upto(10) do |time| %>
          <td id="T<%= dow %>-t<%= time %>"></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<script><%= render "class_arrange.js" %></script>
